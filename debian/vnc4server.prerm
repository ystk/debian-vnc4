#!/bin/sh -e

if [ "$1" = "remove" ] ; then
    BIN=/usr/bin
    update-alternatives --remove \
	vncserver $BIN/vnc4server
    update-alternatives --remove \
	Xvnc $BIN/Xvnc4
    update-alternatives --remove \
	x0vncserver $BIN/x0vnc4server
    update-alternatives --remove \
	vncpasswd $BIN/vnc4passwd
    update-alternatives --remove \
	vncconfig $BIN/vnc4config
fi

#DEBHELPER#

exit 0
